#!/usr/bin/env python

import numpy as np

class Frame:
    def __init__(self, frame_id, kpts, descs):
        self.frame_id = frame_id
        self.kpts = kpts
        self.descs = descs
        #self.p2ds = np.float32([kpt.pt for kpt in kpts])

    def set_matches(matches):
        self.matches = matches

    def set_motion(T):
        self.T = T
        
    def set_structure(p3ds):
        self.p3ds = p3ds
